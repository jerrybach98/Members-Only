<h1>Members Only</h1>
<% if user_signed_in? %> 
 <div> Welcome </div> 
  <%= button_to "Sign out", destroy_user_session_path, method: :delete %> 
  <%= button_to "Create Posts", new_post_path, method: :get %> 
  <h1>Posts</h1>
  <% @posts.each do |post| %>
    <h2><%= '- ' + post.title %></h2>
    <p><%= 'Author: ' + post.user.username %></p>
    <p><%= post.body %></p>
  <% end %>
<% else %>
  <%= button_to "Sign in", new_user_session_path %> 
  <% @posts.each do |post| %>
    <h2><%= '- ' + post.title %></h2>
    <p><%= post.body %></p>
  <% end %>
<% end %>